<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="appium-chrome-example" type="CucumberJavaRunConfigurationType" factoryName="Cucumber java">
    <option name="FILE_PATH" value="$PROJECT_DIR$/src/test/resources/features" />
    <option name="GLUE" value="${groupId}.step_definitions" />
    <option name="MAIN_CLASS_NAME" value="io.cucumber.core.cli.Main" />
    <module name="${artifactId}" />
    <option name="PROGRAM_PARAMETERS" value="--plugin org.jetbrains.plugins.cucumber.java.run.CucumberJvm4SMFormatter --tags &quot;not @no_appium&quot; --plugin &quot;json:target/cucumber-report/json/report.json&quot; --plugin &quot;html:target/cucumber-report/html&quot;" />
    <shortenClasspath name="NONE" />
    <option name="VM_PARAMETERS" value="-Dbrowser=appium_android_device -DbaseUrl=&quot;https://www.wikipedia.org&quot; -Dselenium.grid=http://localhost:4723 -Ddevice.id=!your_Device-ID_here! -Dbrowser.version=107" />
    <option name="WORKING_DIRECTORY" value="$MODULE_WORKING_DIR$" />
    <method v="2">
      <option name="Maven.BeforeRunTask" enabled="true" file="$PROJECT_DIR$/pom.xml" goal="install -DskipTests" />
    </method>
  </configuration>
</component>